<h1>Clocks</h1>
<table class="table table-stripped">
  <tbody>
    {{#each clocks}}
    <tr>
      <td>{{id}}</td>
      <td>{{model}}</td>
      <td>{{type}}</td>
      <td><a href="/clocks/{{id}}" class="btn btn-primary">Show</a></td>
      <td><a href="/clocks/{{id}}/edit" class="btn btn-secondary">Edit</a></td>
      <form method="POST" action="/clocks/{{id}}?_method=DELETE">
      <td><input type="submit" value="Delete" class="btn btn-danger"/></td>
      </form>
    </tr>
    {{/each}}
  </tbody>
</table>
